{
  "description": "Limit the allowed number of open tabs",
  "homepage_url": "https://github.com/Sora185/limit-max-tabs",
  "manifest_version": 2,
  "name": "LimitMaxTabs",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "32": "tab.svg"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Limit Max Tabs",
    "default_icon": {
        "32": "tab.svg"
    }
  },
  "version": "1.0",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "57.0a1",
      "id": "limit-max-tabs@Sora185"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "chrome_style": true
  }
}
